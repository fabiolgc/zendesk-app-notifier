{
  "name": "Notify",
  "author": {
    "name": "Fábio Gonçalves",
    "email": "fabio@zendesk.com",
    "url": "https://www.github.com"
  },
  "defaultLocale": "pt-br",
  "private": true,
  "singleInstall": true,
  "location": {
    "support": {
      "background": {
        "autoLoad": true,
        "url": "assets/iframe.html"
      }
    }
  },
  "parameters": [
    {
      "name": "debug",
      "type": "checkbox"
    },
    {
      "name": "messageType",
      "type": "text",
      "required": true,
      "default": "alert"
    },
    {
      "name": "messageTime",
      "type": "number",
      "required": true,
      "default": "10000"
    }
  ],
  "version": "1.0",
  "frameworkVersion": "2.0"
}
